<p id="notice"><%= notice %></p>

<%= form_for(@student, url: "/valid") do |f| %>
<div class="field">
    <%= f.label :Username  %>
    <%= f.text_field :username  %>
  </div>
<br>
  <div class="field">
    <%= f.label :Password %>
    <%= f.password_field :password %>
  </div>
<br>
  <div class="actions">
    <%= f.submit "Login" %>
 </div> 
 <% end %>
 <ul>
  <li><a href="/form">No Account?Sign up</a></li>
</ul>
